{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "jsx-a11y/img-has-alt": "off",
        "import/no-named-default": "off",
        "react/jsx-tag-spacing": "off",
        "react/no-array-index-key": "off",
        "react/require-default-props": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 100
            }
        ]
    }
}